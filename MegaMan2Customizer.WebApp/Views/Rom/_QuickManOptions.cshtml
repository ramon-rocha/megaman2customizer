@model QuickManOptionsViewModel

@{
    this.Layout = "_RobotMasterLayout";
}

<div class="row">

    <partial name="_CommonRobotMasterOptions" />

    <div class="col-lg-3 form-group">
        <label asp-for="RunSpeed"></label>
        <input class="form-control" asp-for="RunSpeed" />
    </div>
    <div class="col-lg-3 form-group">
        <label asp-for="RunDuration"></label>
        <input class="form-control" asp-for="RunDuration" />
    </div>
    <div class="col-lg-3 form-group">
        <label asp-for="Jump1Height"></label>
        <input class="form-control" asp-for="Jump1Height" />
    </div>
    <div class="col-lg-3 form-group">
        <label asp-for="Jump2Height"></label>
        <input class="form-control" asp-for="Jump2Height" />
    </div>
    <div class="col-lg-3 form-group">
        <label asp-for="Jump3Height"></label>
        <input class="form-control" asp-for="Jump3Height" />
    </div>
    <div class="col-lg-3 form-group">
        <label asp-for="ProjectileCount"></label>
        <input class="form-control" asp-for="ProjectileCount" />
    </div>
    <div class="col-lg-3 form-group">
        <label asp-for="ProjectileLaunchSpeed"></label>
        <input class="form-control" asp-for="ProjectileLaunchSpeed" />
    </div>
    <div class="col-lg-3 form-group">
        <label asp-for="ProjectileReturnDelay"></label>
        <input class="form-control" asp-for="ProjectileReturnDelay" />
    </div>
    <div class="col-lg-3 form-group">
        <label asp-for="ProjectileReturnSpeed"></label>
        <input class="form-control" asp-for="ProjectileReturnSpeed" />
    </div>
</div>