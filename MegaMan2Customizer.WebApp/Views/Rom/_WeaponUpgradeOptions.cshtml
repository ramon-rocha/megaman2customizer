@model WeaponUpgradeOptionsViewModel

<div class="card">
    <div class="card-header">
        <h2 class="panel-title"><a class="collapsed" data-toggle="collapse" href="#collapseWeapons">Weapon Upgrades</a></h2>
    </div>
    <div id="collapseWeapons" class="collapse card-body">

        <div class="card">
            <div class="card-header">
                <h3 class="panel-title"><a class="collapsed" data-toggle="collapse" href="#collapseAtomicFire">Atomic Fire</a></h3>
            </div>
            <div id="collapseAtomicFire" class="collapse card-body">
                <partial name="_CommonWeaponOptions" for="AtomicFire" />
                <div class="row">
                    <div class="col-lg-3 form-group">
                        <label asp-for="AtomicFire.ProjectileSpeed"></label>
                        <input class="form-control" asp-for="AtomicFire.ProjectileSpeed" />
                    </div>
                    <div class="col-lg-3 form-group">
                        <label asp-for="AtomicFire.Level1AmmoUsed"></label>
                        <input class="form-control" asp-for="AtomicFire.Level1AmmoUsed" />
                    </div>
                    <div class="col-lg-3 form-group">
                        <label asp-for="AtomicFire.Level2AmmoUsed"></label>
                        <input class="form-control" asp-for="AtomicFire.Level2AmmoUsed" />
                    </div>
                    <div class="col-lg-3 form-group">
                        <label asp-for="AtomicFire.Level3AmmoUsed"></label>
                        <input class="form-control" asp-for="AtomicFire.Level3AmmoUsed" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-3 form-group">
                        <label asp-for="AtomicFire.Level2ChargeTime"></label>
                        <input class="form-control" asp-for="AtomicFire.Level2ChargeTime" />
                    </div>
                    <div class="col-lg-3 form-group">
                        <label asp-for="AtomicFire.Level3ChargeTime"></label>
                        <input class="form-control" asp-for="AtomicFire.Level3ChargeTime" />
                    </div>
                </div>
            </div>
        </div>

        <div class="card">
            <div class="card-header">
                <h3 class="panel-title"><a class="collapsed" data-toggle="collapse" href="#collapseAirShooter">Air Shooter</a></h3>
            </div>
            <div id="collapseAirShooter" class="collapse card-body">
                <partial name="_CommonWeaponOptions" for="AirShooter" />
                <div class="row">
                    <div class="col-lg-3 form-group">
                        <label asp-for="AirShooter.ProjectileCount"></label>
                        <input class="form-control" asp-for="AirShooter.ProjectileCount" />
                    </div>
                    <div class="col-lg-3 form-group">
                        <label asp-for="AirShooter.Projectile1HorizontalSpeed"></label>
                        <input class="form-control" asp-for="AirShooter.Projectile1HorizontalSpeed" />
                    </div>
                    <div class="col-lg-3 form-group">
                        <label asp-for="AirShooter.Projectile2HorizontalSpeed"></label>
                        <input class="form-control" asp-for="AirShooter.Projectile2HorizontalSpeed" />
                    </div>
                    <div class="col-lg-3 form-group">
                        <label asp-for="AirShooter.Projectile3HorizontalSpeed"></label>
                        <input class="form-control" asp-for="AirShooter.Projectile3HorizontalSpeed" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-3 form-group">
                        <label asp-for="AirShooter.VerticalAcceleration"></label>
                        <input class="form-control" asp-for="AirShooter.VerticalAcceleration" />
                    </div>
                    <div class="col-lg-3 form-group">
                        <label asp-for="AirShooter.AmmoUsed"></label>
                        <input class="form-control" asp-for="AirShooter.AmmoUsed" />
                    </div>
                </div>
            </div>
        </div>

        <div class="card">
            <div class="card-header">
                <h3 class="panel-title"><a class="collapsed" data-toggle="collapse" href="#collapseLeafShield">Leaf Shield</a></h3>
            </div>
