                    </div>
                    <div class="col-lg-3 form-group">
                        <label asp-for="LeafShield.ProjectileSpeed"></label>
                        <input class="form-control" asp-for="LeafShield.ProjectileSpeed" />
                    </div>
                </div>
            </div>
        </div>

